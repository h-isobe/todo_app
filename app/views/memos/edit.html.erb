<div class="col-xs-12 text-center">

  <h1>メモ編集</h1>

  <%= form_with model: @memo do |f| %>
    <div><%= f.label :title %></div>
    <div><%= f.text_field :title %></div>
    <div><%= f.label :content %></div>
    <div><%= f.text_area :content, :size => '45×45' %></div>
    <div><%= f.submit '編集', class: "btn btn-success" %></div>
  <% end %>

  <div class="memo-edit-back">
    <%= link_to '戻る', :back, class: "btn btn-info" %>
  </div>

</div>
